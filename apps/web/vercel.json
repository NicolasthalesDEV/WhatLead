{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "cd ../.. && pnpm install",
  "buildCommand": "cd ../.. && SKIP_ENV_VALIDATION=true pnpm turbo run build --filter=@wacrm/web",
  "outputDirectory": ".next",
  "regions": ["gru1"],
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "true"
    }
  }
}
